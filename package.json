{
  "name": "hibiscus.js",
  "description": "Native Angular directives for Bootstrap4",
  "keywords": [
    "angular",
    "angular2",
    "angular4",
    "angular5",
    "webpack",
    "typescript"
  ],
  "author": "Kimi Liu <839536@qq.com>",
  "homepage": "https://github.com/839536/hibiscus.js",
  "license": "MIT",
  "main": "index.js",
  "typings": "index.d.ts",
  "private": true,
  "scripts": {
    "npm:pre": "gulp prepublish",
    "build:aot:prod": "npm run clean:dist && npm run clean:aot && cross-env BUILD_AOT=1 npm run webpack -- --config bin/webpack.prod.js  --progress --profile --bail",
    "build:aot": "npm run build:aot:prod",
    "build:dev": "npm run clean:dist && npm run webpack -- --config bin/webpack.dev.js --progress --profile",
    "build:prod": "npm run clean:dist && npm run webpack -- --config bin/webpack.prod.js  --progress --profile --bail",
    "build:docker": "npm run build:prod && docker build -t angular2-webpack-start:latest .",
    "build": "npm run build:dev",
    "ci:aot": "npm run lint && npm run test && npm run build:aot && npm run e2e",
    "ci:jit": "npm run lint && npm run test && npm run build:prod && npm run e2e",
    "ci:nobuild": "npm run lint && npm test && npm run e2e",
    "ci:testall": "npm run lint && npm run test && npm run build:prod && npm run e2e && npm run build:aot && npm run e2e",
    "ci:travis": "npm run lint && npm run test && npm run build:aot && npm run e2e:travis",
    "ci": "npm run ci:testall",
    "clean:dll": "npm run rimraf -- dll",
    "clean:aot": "npm run rimraf -- compiled",
    "clean:dist": "npm run rimraf -- dist",
    "clean:install": "npm set progress=false && npm install",
    "clean": "npm cache clean --force && npm run rimraf -- node_modules doc coverage dist compiled dll",
    "docker": "docker",
    "docs": "npm run typedoc -- --options typedoc.json --exclude '**/*.spec.ts' ./src/",
    "docs:compodoc": "compodoc -p tsconfig.json",
    "docs:compodoc:serve": "compodoc -p tsconfig.json -s  -d ./dist/doc/",
    "docs:compodoc:serve:watch": "compodoc -p tsconfig.json -s -w  -d ./dist/doc/",
    "docs:compodoc:serve:gen": "compodoc ./src/app/exports/ -p tsconfig.json -d ./dist/doc/ --hideGenerator",
    "e2e:live": "npm-run-all -p -r server:prod:ci protractor:live",
    "e2e:travis": "npm-run-all -p -r server:prod:ci protractor:delay",
    "e2e": "npm-run-all -p -r server:prod:ci protractor",
    "protractor": "protractor",
    "protractor:delay": "sleep 3 && npm run protractor",
    "protractor:live": "protractor --elementExplorer",
    "server:dev:hmr": "npm run server:dev -- --hotOnly",
    "server:dev": "npm run webpack-dev-server -- --config bin/webpack.dev.js --open --progress --profile --watch --content-base src/",
    "server:prod": "http-server dist -c-1 --cors",
    "server": "npm run server:dev",
    "mock": "node ./bin/mock.server.js",
    "start": "npm run server:dev",
    "start:hmr": "npm run server:dev:hmr",
    "start:mock": "npm-run-all -p -r mock server:dev",
    "start:aot": "npm run server:aot:dev",
    "watch:dev:hmr": "npm run watch:dev -- --hot",
    "watch:dev": "npm run build:dev -- --watch",
    "watch:prod": "npm run build:prod -- --watch",
    "watch:test": "npm run test -- --auto-watch --no-single-run",
    "watch": "npm run watch:dev",
    "lint": "npm run tslint \"src/**/*.ts\"",
    "node": "node",
    "rimraf": "rimraf",
    "test": "npm run lint && karma start",
    "tslint": "tslint",
    "typedoc": "typedoc",
    "version": "npm run build",
    "webpack": "node --max_old_space_size=4096 node_modules/webpack/bin/webpack.js",
    "webdriver:start": "npm run webdriver-manager start",
    "webdriver:update": "webdriver-manager update",
    "webdriver-manager": "webdriver-manager",
    "webpack-dev-server": "node --max_old_space_size=4096 node_modules/webpack-dev-server/bin/webpack-dev-server.js"
  },
  "dependencies": {
    "@angular/animations": "~5.1.0",
    "@angular/common": "~5.1.0",
    "@angular/compiler": "~5.1.0",
    "@angular/core": "~5.1.0",
    "@angular/forms": "~5.1.0",
    "@angular/http": "~5.1.0",
    "@angular/platform-browser": "~5.1.0",
    "@angular/platform-browser-dynamic": "~5.1.0",
    "@angular/cdk": "^5.0.0",
    "@angular/router": "~5.1.0",
    "core-js": "^2.4.1",
    "date-fns": "^1.27.2",
    "echarts": "^3.4.0",
    "highlight.js": "^9.12.0",
    "ie-shim": "^0.1.0",
    "jquery": "^3.3.1",
    "nprogress": "^0.2.0",
    "perfect-scrollbar": "^1.2.0",
    "rxjs": "^5.4.3",
    "screenfull": "^3.2.2",
    "zone.js": "~0.8.18",
    "ngx-pagination": "^3.1.0"
  },
  "devDependencies": {
    "@angular/compiler-cli": "~5.1.0",
    "@angular/platform-server": "~5.1.0",
    "@compodoc/compodoc": "^1.0.9",
    "@types/hammerjs": "^2.0.34",
    "@types/jasmine": "2.5.45",
    "@types/node": "^7.0.56",
    "angular2-template-loader": "^0.6.2",
    "assets-webpack-plugin": "^3.5.1",
    "awesome-typescript-loader": "~3.1.2",
    "chalk": "2.1.0",
    "codelyzer": "~4.0.1",
    "copy-webpack-plugin": "^4.5.1",
    "cross-env": "^5.1.4",
    "css-loader": "^0.28.10",
    "exports-loader": "^0.6.4",
    "expose-loader": "^0.7.5",
    "extract-text-webpack-plugin": "~3.0.0",
    "file-loader": "^0.11.1",
    "gulp": "^3.9.1",
    "gulp-clean": "^0.3.2",
    "gulp-clean-css": "^3.9.3",
    "gulp-ejs": "^3.1.2",
    "gulp-html-minifier": "^0.1.8",
    "gulp-if": "^2.0.2",
    "gulp-ignore": "^2.0.2",
    "gulp-inline-ng2-template": "^4.0.0",
    "gulp-insert-lines": "0.0.4",
    "gulp-optimize-js": "^1.1.0",
    "gulp-sass": "^3.2.0",
    "gulp-stubby-server": "^0.2.0",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^2.30.1",
    "http-server": "^0.9.0",
    "immutable": "^3.8.1",
    "inline-manifest-webpack-plugin": "^3.0.1",
    "istanbul-instrumenter-loader": "2.0.0",
    "jasmine-core": "^2.99.1",
    "karma": "^1.6.0",
    "karma-chrome-launcher": "^2.0.0",
    "karma-coverage": "^1.1.1",
    "karma-jasmine": "^1.1.0",
    "karma-mocha-reporter": "^2.2.3",
    "karma-remap-coverage": "^0.1.5",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-webpack": "^2.0.13",
    "markdown-loader": "^2.0.0",
    "ng-router-loader": "^2.1.0",
    "node-sass": "^4.8.1",
    "npm-run-all": "^4.0.2",
    "preload-webpack-plugin": "^1.2.2",
    "protractor": "^5.3.0",
    "raw-loader": "0.5.1",
    "rimraf": "~2.6.1",
    "run-sequence": "^2.1.0",
    "sass-loader": "^6.0.7",
    "source-map-loader": "^0.2.1",
    "style-loader": "^0.18.1",
    "to-string-loader": "^1.1.5",
    "ts-node": "^3.3.0",
    "tslib": "^1.9.0",
    "tslint": "~5.0.0",
    "typedoc": "^0.7.1",
    "typescript": ">=2.4.2 <2.5",
    "url-loader": "^0.5.8",
    "webpack": "^3.11.0",
    "webpack-dev-middleware": "^1.12.1",
    "webpack-dev-server": "~2.9.4",
    "webpack-merge": "^4.1.2"
  },
  "engines": {
    "node": ">= 6.0 < 7.0",
    "npm": ">= 3.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/839536/hibiscus.js.git"
  }
}
